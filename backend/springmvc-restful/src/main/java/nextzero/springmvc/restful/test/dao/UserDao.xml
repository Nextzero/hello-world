<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="nextzero.springmvc.restful.test.dao.UserDao">
    <select id="getUserList"  resultType="nextzero.springmvc.restful.test.entity.User">
        SELECT * FROM test.user;
    </select>

    <select id="getUserById"  parameterType="string" resultType="nextzero.springmvc.restful.test.entity.User">
        SELECT * FROM test.user WHERE userId=#{userId};
    </select>
</mapper>